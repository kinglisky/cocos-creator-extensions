{
    "name": "@cocos-ext/inspector",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "preview": "vite preview"
    },
    "main": "dist/main.js",
    "module": "dist/main.js",
    "panels": {
        "default": {
            "title": "插件自定义面板",
            "type": "dockable",
            "main": "./dist/panel.js",
            "icon": "./dist/icon.png"
        }
    },
    "contributions": {
        "menu": [
            {
                "path": "i18n:menu.panel/inspector",
                "label": "i18n:simple-vue.open_panel",
                "message": "open-panel"
            }
        ],
        "messages": {
            "open-panel": {
                "methods": [
                    "openPanel"
                ]
            }
        }
    },
    "dependencies": {
        "vue": "^3.2.25"
    },
    "devDependencies": {
        "@cocos-pkg/types": "workspace:^1.0.0",
        "@vitejs/plugin-vue": "^2.3.1",
        "typescript": "^4.5.4",
        "vite": "^2.9.7",
        "vue-tsc": "^0.34.7"
    }
}
