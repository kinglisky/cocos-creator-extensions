{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "Active": {
      "type": "boolean"
    },
    "Locked": {
      "type": "boolean"
    },
    "Name": {
      "type": "string"
    },
    "Position": {
      "type": "object",
      "properties": {
        "x": {
          "type": "integer"
        },
        "y": {
          "type": "integer"
        },
        "z": {
          "type": "integer"
        }
      },
      "required": ["x", "y", "z"]
    },
    "Rotation": {
      "type": "object",
      "properties": {
        "x": {
          "type": "integer"
        },
        "y": {
          "type": "integer"
        },
        "z": {
          "type": "integer"
        }
      },
      "required": ["x", "y", "z"]
    },
    "Scale": {
      "type": "object",
      "properties": {
        "x": {
          "type": "integer"
        },
        "y": {
          "type": "integer"
        },
        "z": {
          "type": "integer"
        }
      },
      "required": ["x", "y", "z"]
    },
    "Layer": {
      "type": "integer"
    }
  },
  "required": [
    "Active",
    "Locked",
    "Name",
    "Position",
    "Rotation",
    "Scale",
    "Layer"
  ]
}
